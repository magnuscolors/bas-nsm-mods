<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
           <record id="product_category_form_view_ext" model="ir.ui.view">
                <field name="name">product.category.form.ext</field>
                <field name="model">product.category</field>
                <field name="inherit_id" ref="product.product_category_form_view"/>
                <field name="arch" type="xml">
                    <field name="type" position="after">
                        <field name="supportal" />
                    </field>
                </field>
           </record>
            <record id="product_category_tree_view_inherit" model="ir.ui.view">
                <field name="name">product.category.tree.inherit</field>
                <field name="model">product.category</field>
                <field name="field_parent">child_id</field>
                <field name="inherit_id" ref="product.product_category_tree_view"/>
                <field name="arch" type="xml">
                    <field name="name" position="replace" >
                            <field name="complete_name"/>
                            <field name="supportal"/>
                    </field>
                </field>
             </record>
    </data>
</openerp>
